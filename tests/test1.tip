// Allows farmer to choose safe temperatures for hot mash feeding.
main() {
    var hotmash,temp,result;
    hotmash = Device(3, 1);
    temp = 1;
    result = 11;
    if (temp > 101) {
        // safe temperature, so heat the mash:
        hotmash.write(temp);
        result = 0;
    }
    output(hotmash.read);
    return result;
}
